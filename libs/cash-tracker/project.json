{
  "name": "cash-tracker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cash-tracker/src",
  "projectType": "library",
  "tags": ["cash", "tracker", "finance"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/cash-tracker",
        "main": "libs/cash-tracker/src/index.ts",
        "tsConfig": "libs/cash-tracker/tsconfig.lib.json",
        "assets": ["libs/cash-tracker/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/cash-tracker/jest.config.ts"
      }
    }
  }
}
